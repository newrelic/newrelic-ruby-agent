# frozen_string_literal: true

RESQUE_VERSIONS = [
  [nil],
  ['2.2.0', 2.4],
  ['1.27.0']
]

def gem_list(resque_version = nil)
  <<~RB
  gem 'resque'#{resque_version}
  
  RB
end

create_gemfiles(RESQUE_VERSIONS)
