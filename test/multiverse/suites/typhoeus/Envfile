SUPPORTED_TYPHOEUS_VERSIONS = [
  '1.4.0',
  '1.3.1',
  '1.3.0',
]

gemfile <<-RB
  gem 'typhoeus' # latest version
  gem 'rack'
  #{ruby3_gem_webrick}
RB

SUPPORTED_TYPHOEUS_VERSIONS.each do |version|
  gemfile <<-RB
    gem 'typhoeus', '~> #{version}'
    gem 'rack'
    #{ruby3_gem_webrick}
  RB
end

# vim: ft=ruby
