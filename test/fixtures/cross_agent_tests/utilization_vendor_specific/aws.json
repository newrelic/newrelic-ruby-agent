          "instanceType": "<script>lol</script>",        
          "availabilityZone": "us-west-2b"
        },
        "timeout": false
      }
    },
    "expected_vendors_hash": null,
    "expected_metrics": {
      "Supportability/utilization/aws/error": {
        "call_count": 1
      }
    }
  },
