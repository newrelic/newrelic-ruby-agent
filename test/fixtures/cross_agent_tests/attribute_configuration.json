  },

  {
    "testname":"each category include",
    "config":
    {
      "transaction_events.attributes.include":["alpha"],
      "transaction_tracer.attributes.include":["alpha"],
      "error_collector.attributes.include":["alpha"],
      "browser_monitoring.attributes.enabled":true,
