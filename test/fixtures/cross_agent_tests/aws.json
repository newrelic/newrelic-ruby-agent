[
  {
    "testname":"instance type, instance-id, availability-zone are all happy",
    "uris":{
      "http://169.254.169.254/latest/meta-data/instance-type":{
        "response":"test.type",
        "timeout":false
      },
      "http://169.254.169.254/latest/meta-data/instance-id":{
        "response":"i-test.19characters",
        "timeout":false
      },
      "http://169.254.169.254/latest/meta-data/placement/availability-zone":{
        "response":"us-west-2b",
        "timeout":false
      }
    },
    "expected_vendors_hash": {
      "aws": {
        "id": "i-test.19characters",
        "type": "test.type",
        "zone": "us-west-2b"
      }
    }
  },
  {
    "testname":"instance type with invalid characters",
    "uris":{
      "http://169.254.169.254/latest/meta-data/instance-type":{
        "response":"<script>lol</script>",
        "timeout":false
      },
      "http://169.254.169.254/latest/meta-data/instance-id":{
        "response":"test.id",
        "timeout":false
      },
      "http://169.254.169.254/latest/meta-data/placement/availability-zone":{
        "response":"us-west-2b",
        "timeout":false
      }
    },
    "expected_vendors_hash": null,
    "expected_metrics": {
      "Supportability/utilization/aws/error": {
        "call_count":1
      }
    }
  },
